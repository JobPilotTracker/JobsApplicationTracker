{
  "name": "jobsapplicationtracker",
  "private": true,
  "version": "0.1",
  "workspaces": [
    "apps/backend",
    "apps/frontend"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "dev:frontend": "npm --workspace apps/frontend run dev",
    "dev:backend": "npm --workspace apps/backend run dev"
  },
  "keywords": [
    "job",
    "application",
    "tracker",
    "monorepo",
    "react",
    "node",
    "express",
    "typescript"
  ],
  "author": "JobPilotTracker",
  "license": "ISC",
  "description": "Monorepo for the Jobs Application Tracker application, managing frontend and backend services.",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^17.2.0"
  },
  "dependencies": {
    "dotenv-mono": "^1.4.0"
  }
}
